<?xml version="1.0"?>
<robot name="grip">

    <link name="base">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>

    </link>

    
    <link name="grip_body">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.075"/>
            <geometry>
                <box size="0.15 0.05 0.2"/>
            </geometry>
            <material name="yellow">
                <color rgba="1 1 0 1"/>
            </material>
        </visual>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.0067" ixy="0.0" ixz="0.0" iyy="0.0067" iyz="0.0" izz="0.0035"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.075"/>
            <geometry>
                <box size="0.15 0.05 0.2"/>
            </geometry>
        </collision>
    </link>

    <joint name="base_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base"/>
        <child link="grip_body"/>
        
    </joint>
    
    <link name="right_finger">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.06"/>
            <geometry>
                <box size="0.03 0.05 0.12"/>
            </geometry>
            <material name="yellow">
                <color rgba="1 1 0 1"/>
            </material>
            
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.06"/>
            <geometry>
                <box size="0.03 0.05 0.12"/>
            </geometry>     
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.0067" ixy="0.0" ixz="0.0" iyy="0.0067" iyz="0.0" izz="0.0035"/>
        </inertial>
    </link>

    <joint name="slide_right" type="prismatic">
        <origin xyz="0.025 0 0.18" rpy="0 0 0 "/>
        <parent link= "grip_body"/>
        <child link = "right_finger"/>
        <axis xyz="1 0 0"/>
        <limit effort="30" velocity="1.0" lower="0" upper="0.05" />
        <dynamics damping="0.0" friction="0.0"/>
    </joint>

    <link name="left_finger">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.06"/>
            <geometry>
                <box size="0.03 0.05 0.12"/>
            </geometry>
            <material name="yellow">
                <color rgba="1 1 0 1"/>
            </material>
        </visual>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.0067" ixy="0.0" ixz="0.0" iyy="0.0067" iyz="0.0" izz="0.0035"/>
        </inertial>

        <collision>
            <origin rpy="0 0 0" xyz="-0 0 0.06"/>
            <geometry>
                <box size="0.03 0.05 0.12"/>
            </geometry>
        </collision>
        
    </link>

    <joint name="slide_left" type="prismatic">
        <origin xyz="-0.025 0 0.18" rpy="0 0 0 "/>
        <parent link= "grip_body"/>
        <child link = "left_finger"/>
        <axis xyz="-1 0 0"/>
        <limit effort="30" velocity="1.0" lower="0" upper="0.05" />
        <dynamics damping="0.0" friction="0.0"/>
    </joint>



    
</robot>
